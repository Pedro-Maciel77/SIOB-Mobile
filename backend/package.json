{
  "name": "siob-mobile",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm migration:generate src/migrations/CreateTables -d src/config/database.ts",
    "migration:run": "npm run typeorm migration:run -d src/config/database.ts",
    "migration:revert": "npm run typeorm migration:revert -d src/config/database.ts",
    "migration:create": "npm run typeorm migration:create src/migrations/CreateTables",
    "seed": "ts-node src/seeds/seed.ts",
    "db:reset": "npm run migration:revert && npm run migration:run && npm run seed"
  },
  "dependencies": {
    "typeorm": "^0.3.20",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/bcrypt": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}